{
  "name": "gdbots/iam",
  "description": "Php library that provides implementations for gdbots:iam schemas.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "gdbots/ncr": "~0.1",
    "gdbots/pbjx": "~2.0",
    "psr/log": "~1"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "gdbots/acme-schemas": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Iam\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Iam\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
